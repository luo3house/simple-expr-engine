{
  "name": "simple-expr-engine",
  "version": "1.0.0",
  "description": "Simple Rule & Expression Interpreter",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
