    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "rollup-plugin-terser": "^7.0.2"
  }
